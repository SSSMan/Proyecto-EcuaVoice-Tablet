                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/fonemas.png"/>
                </Button.Background>
            </Button>
            <Button x:ConnectionId='3' Name="btnTrabalenguas" Content="" Margin="939,197,0,378"                               BorderBrush="{x:Null}" Width="232" Height="193">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/trabalengua.png"/>
                </Button.Background>
            </Button>
            <Button x:ConnectionId='4' Name="btnRimas" Content="" Margin="939,412,0,163"                        BorderBrush="{x:Null}" Width="232" Height="193" RenderTransformOrigin="0.502,0.595">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/Rimas.png"/>

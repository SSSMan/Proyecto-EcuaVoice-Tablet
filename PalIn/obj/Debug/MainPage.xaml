                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/pares.png"/>
                </Button.Background>
            </Button>
            <Button x:ConnectionId='7' x:Name="btnSilabasCompuestas" Content="" Margin="706,412,0,163"                                    Height="193" Width="234" BorderBrush="{x:Null}">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/silabascompuestas.png"/>
                </Button.Background>
            </Button>
            <Button x:ConnectionId='8' x:Name="btnLector_Copy" Content="" Margin="475,408,0,167"                         BorderBrush="{x:Null}" Width="232" Height="193">
                <Button.Background>
                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/deletreo.png"/>

                    <ImageBrush Stretch="Fill" ImageSource="Imagenenes/lector.png"/>
                </Button.Background>
            </Button>
            <Button x:ConnectionId='2' x:Name="btnfonemas" Content="" Margin="475,193,0,382" BorderBrush="{x:Null}" Width="232" Height="193"                         >
